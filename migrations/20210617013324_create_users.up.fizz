create_table("users") {
  t.Column("id", "integer", {primary: true})
  t.Column("first_name", "string", {"size": 255})
  t.Column("last_name", "string", {"size": 255})
  t.Column("email", "string", {"size": 255})
  t.Column("username", "string", {"size": 255})
  t.Column("password_hash", "string", {"size": 255})
  t.Column("token", "string", {"size": 255})
}
